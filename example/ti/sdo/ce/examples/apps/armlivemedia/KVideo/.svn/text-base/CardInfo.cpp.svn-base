#include "CardInfo.h"

using namespace KVideo;

CardInfo::CardInfo() :
		m_port(0) {
}

CardInfo::~CardInfo() {
}

string CardInfo::getIP() {
	return m_ip;
}

bool CardInfo::setIP(const string& strIP) {
	m_ip = strIP;
	return true;
}

int CardInfo::getPort() {
	return m_port;
}

bool CardInfo::setPort(int port) {
	m_port = port;
	return true;
}

string CardInfo::getUsername() {
	return m_username;
}

bool CardInfo::setUsername(const string& username) {
	m_username = username;
	return true;
}

string CardInfo::getPassword() {
	return m_password;
}

bool CardInfo::setPassword(const string& password) {
	m_password = password;
	return true;
}

string CardInfo::getCardDescription() {
	return m_cardname;
}

bool CardInfo::setCardDescription(const string& description) {
	m_cardname = description;
	return true;
}
